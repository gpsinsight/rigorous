{
  "name": "rigorous",
  "version": "0.2.13",
  "description": "Mocking and testing utilities for developing APIs with Swagger/OpenAPI",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "scripts": {
    "prebuild": "npm run lint && rm -rf lib/*",
    "build": "tsc",
    "lint": "tslint -c tslint.json -e 'node_modules/**/*' '**/*.ts'",
    "pretest": "rm -rf src/tests/*.g.* && npm run build && node ./lib/tests/build-tests.js",
    "test": "NODE_ENV=test nyc mocha --require source-map-support/register --require ts-node/register --recursive './src/**/*.tests.*'",
    "prepack": "npm run build"
  },
  "keywords": [
    "testing",
    "mocking",
    "swagger",
    "openapi",
    "api",
    "apis"
  ],
  "author": "",
  "license": "MIT",
  "files": [
    "/lib"
  ],
  "dependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/prettier": "^1.16.1",
    "@types/uuid": "^3.4.4",
    "body-parser": "^1.18.3",
    "chance": "^1.0.18",
    "express": "^4.16.4",
    "json-refs": "^3.0.12",
    "openapi-router": "0.0.6",
    "prettier": "^1.16.4",
    "randexp": "^0.5.3",
    "swagger-ui-express": "^4.0.2",
    "typescript": "^3.3.3",
    "uuid": "^3.3.2"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/chance": "^1.0.1",
    "@types/mocha": "^5.2.6",
    "@types/node": "^11.9.4",
    "chai": "^4.2.0",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "source-map-support": "^0.5.10",
    "ts-node": "^8.0.2",
    "tslint": "^5.12.1"
  }
}
